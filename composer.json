{
    "name": "j6s/static-file-cache",
    "type": "neos-plugin",
    "config": {
        "vendor-dir": "Packages/Libraries"
    },
    "require": {
        "neos/neos": "~4.2",
        "php": "~7.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpstan/phpstan-shim": "^0.11.1",
        "squizlabs/php_codesniffer": "^3.4",
        "phpunit/phpunit": "^7.5.4"
    },
    "autoload": {
        "psr-4": {
            "J6s\\StaticFileCache\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "J6s\\StaticFileCache\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "J6s.StaticFileCache"
        }
    }
}
